---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
  <channel>
    <title>{{ site.default_title }}</title>
    <description>Updates and useful tips for the Fleakr Ruby library.</description>
    <link>http://fleakr.org</link>
    {% for post in site.posts %}
      <item>
        <title>{{ post.title }}</title>
        <description>{{ post.content | xml_escape }}</description>
        <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
        <link>http://fleakr.org/{{ post.id }}</link>
        <guid>http://fleakr.org/{{ post.id }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>